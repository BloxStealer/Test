local function GetThumbnail(imageid)
    local Asset = string.split(imageid, "rbxassetid://")[2]
    local Size = "420x420"
    local Image =
        game:HttpGet(
        "https://thumbnails.roblox.com/v1/assets?assetIds=" ..
            Asset .. "&returnPolicy=PlaceHolder&size=" .. Size .. "&format=png"
    )
    local thumbnail = game.HttpService:JSONDecode(Image).data[1].imageUrl
    return thumbnail
end

